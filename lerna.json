{
  "packages": [
    "services/api-types/*",
    "services/*-service",
    "libs/*"
  ],
  "version": "independent",
  "concurrency": 10,
  "command": {
    "publish": {
      "message": "package publish",
      "npmClient": "npm",
      "registry": "https://npm.pkg.github.com/vladLazurenko",
      "ignoreChanges": [
        "**/*.md",
        "**/example/**",
        "**/test/**",
        "**/__tests__/**",
        "**/package-lock.json"
      ]
    }
  }
}
